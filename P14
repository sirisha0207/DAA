 #include <stdio.h>
#include <string.h>

int main() {
    char str[100];

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    // Remove newline character from fgets input
    str[strcspn(str, "\n")] = '\0';

    // Using strlen() function to find length of string
    int length = strlen(str);

    printf("Length of the string: %d\n", length);

    return 0;
}
